<div class="admin_genres_index_container">
  <h2>ジャンル編集</h2>

  <%= form_with model: @genre,url:admin_genre_path, local:true do |f|%>
    <div class="admin_genres_index_new">
      <%= f.label :name, 'ジャンル名' %>
      <%= f.text_field :name, autofocus: true, autocomplete: "genre" %>
      <%= f.submit '変更を保存',class:"btn btn_green" %>
    </div>
  <% end %>
</div>